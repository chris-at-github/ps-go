tt_content {
	xo_gallery =< lib.contentElement
	xo_gallery {
		templateName = Gallery.html

		dataProcessing {
			10 = TYPO3\CMS\Frontend\DataProcessing\FilesProcessor
			10 {
				as = images
				references.fieldName = tx_xo_file
			}

			100 {
				frameTypeClass.replacement.10 {
					search = ce-xo-gallery
					replace = ce-gallery
				}
			}
		}
	}
}